{
  "name": "@ai-model-sentinel/monorepo",
  "version": "0.1.0-alpha.0",
  "private": true,
  "description": "Enterprise AI Model Monitoring Platform - Monorepo",
  "keywords": [
    "ai-monitoring",
    "mlops",
    "enterprise",
    "monorepo"
  ],
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "lerna run build",
    "test": "lerna run test",
    "lint": "lerna run lint",
    "dev": "lerna run dev --parallel",
