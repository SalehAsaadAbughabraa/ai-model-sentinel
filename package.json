    "clean": "lerna run clean",
    "bootstrap": "lerna bootstrap",
    "docker:build": "docker build -t ai-model-sentinel .",
    "docker:push": "docker push registry/ai-model-sentinel:latest",
    "docker:start": "docker-compose up -d"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.9.0",
    "jest": "^29.7.0",
