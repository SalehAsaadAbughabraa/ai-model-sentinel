    "typescript": "^5.2.2"
  },
  "dependencies": {
    "lucide-react": "^0.542.0"
  },
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.6.7"
  },
  "packageManager": "npm@9.6.7",
