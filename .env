# AI Model Sentinel v2.0.0 - Environment Configuration
# Production Environment Settings

# Database Configuration
DATABASE_URL=postgresql://sentinel:SecurePassword123@postgres:5432/sentinel_db
REDIS_URL=redis://:RedisSecure123!@redis:6379/0

# Security Configuration
SECRET_KEY=your-super-secure-secret-key-change-in-production-123456
JWT_SECRET=your-jwt-secret-key-change-in-production-789012
JWT_ALGORITHM=HS512

# Application Configuration
ENVIRONMENT=production
WORKERS=4
LOG_LEVEL=INFO
ALLOWED_ORIGINS=*

# Monitoring Configuration
SENTRY_DSN=your-sentry-dsn-here
ELASTIC_APM_SERVER_URL=your-apm-server-url

# Security Settings
MAX_FILE_SIZE=104857600  # 100MB in bytes
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=300  # 5 minutes in seconds

# Engine Configuration
QUANTUM_ENGINE_WEIGHT=0.35
ML_ENGINE_WEIGHT=0.30
BEHAVIORAL_ENGINE_WEIGHT=0.20
SIGNATURE_ENGINE_WEIGHT=0.15

# Threat Thresholds
CRITICAL_THRESHOLD=0.8
HIGH_THRESHOLD=0.6
MEDIUM_THRESHOLD=0.4
LOW_THRESHOLD=0.2

# Cache Configuration
REDIS_CACHE_TTL=3600
REDIS_SCAN_TTL=300

# Database Pool Configuration
DB_POOL_SIZE=20
DB_MAX_OVERFLOW=30
DB_POOL_RECYCLE=3600\nB2_KEY_ID=disabled_for_now\nB2_APPLICATION_KEY=disabled_for_now\n